<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.s326197mappe2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddOrderActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/add_order_label"/>
        <activity android:name=".AddFriendActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/add_friend_label"/>
        <activity android:name=".AddRestaurantActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/add_restaurant_label"/>
    </application>

</manifest>